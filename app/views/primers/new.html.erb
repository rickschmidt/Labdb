
<h1>Create a New Primer</h1>

<% form_for(@primer) do |f| %>
  <%= f.error_messages %>
<table border="0" cellspacing="5" cellpadding="5">


<tr><td><b>Primer L: </b><br />
    <%= f.text_field :primerl, :size=>'50' %></td>
	</tr>
	<tr>

	<td><b>Primer H: </b><br />
    <%= f.text_field :primerh, :size=>'50' %></td></tr>
</table>


  
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', primers_path %>