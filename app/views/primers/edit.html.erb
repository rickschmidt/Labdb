<h1>Edit primer</h1>

<% form_for(@primer) do |f| %>
  <%= f.error_messages %>
<table border="0" cellspacing="5" cellpadding="5">


<tr><td>Primer L:<br />
    <%= f.text_field :primerl %></td>
	<td> Primer H:<br />
    <%= f.text_field :primerh %></td></tr>
</table>


  
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', primers_path %>