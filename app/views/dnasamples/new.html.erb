<h1>New DNA Sample</h1>

<% form_for(@dnasample) do |f| %>
  <%= f.error_messages %>

<table border="0" cellspacing="5" cellpadding="5">
	<tr><td>	 
    	<%= f.label :dna_accession, "DNA Accession #" %><br />
	    <%= f.text_field :dna_accession %></td>

		<td>
    	<%= f.label :dnasample_number, "DNA Sample #" %><br />
	    <%= f.text_field :dnasample_number %></td>
	</tr>

  
</table>
 
  <p>
    <%= f.label :date %><br />
    <%= f.date_select :date %>
  </p>
  <p>
    <%= f.label :method %><br />
    <%= f.text_field :method %>
  </p>
  <p>
    <%= f.label :elution_volume %><br />
    <%= f.text_field :elution_volume %>
  </p>
  <p>
    <%= f.label :second_elute %><br />
    <%= f.text_field :second_elute %>
  </p>
  <p>
    <%= f.label :second_elute_volumne %><br />
    <%= f.text_field :second_elute_volumne %>
  </p>
  <p>
    <%= f.label :reextracted %><br />
    <%= f.text_field :reextracted %>
  </p>
  <p>
    <%= f.label :created_by %><br />
    <%= f.text_field :created_by %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', dnasamples_path %>